LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL;
ENTITY LOCKIS
PORT (le:IN STD_LOGIC;
		dd:IN STD_LOGIC_VECTOR(3DOWNTO0);
		qq:OUT STD_LOGIC_VECTOR(3DOWNTO0));
ENDLOCK;
ARCHITECTURE one OFLOCK IS
BEGIN
	PROCESS (le,dd)
		BEGIN
			IF (le=‘1’)THEN
				qq<=dd;
			END IF;
		END PROCESS;
	END one;